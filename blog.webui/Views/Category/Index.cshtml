@model List<Category>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table>
        <tr>
            @foreach (var item in Model)
            {
                <td>@item.CategoryName</td>
            }

        </tr>
    </table>
</body>
</html>
